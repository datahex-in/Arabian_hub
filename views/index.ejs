<body>
  <!-- ========== Start header navbar ========== -->
  <%-include("header.ejs")%>
    <!-- ========== End navbar ========== -->

    <!-- ========== Start home section ========== -->
    <div class="section_home_head">
      <div class="container-fluid">
        <div class="section_home">
          <div class="section_home_left">
            <div class="section_home_content">
              <h2 class="section_title" data-aos="fade-right" data-aos-duration="1000" data-aos-easing="ease-in-out">
                Explore. Plan. Book.
                Your Journey, Your Way.</h2>
              <p class="section_content w-100" data-aos="fade-up" data-aos-duration="1500" data-aos-easing="linear">
                Discover your next adventure, where every click leads to a new
                experience.
                Seamlessly
                create and organize your travel plans, leaving no destination uncharted.</p>
              <div class=" section_home_data">
                <div class="form__group" style="display: flex;justify-content: space-between;">
                  <input type="input" class="form__field" required="true" placeholder="Destinations">
                  <div style="margin-left: -20px;">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                      <path fill-rule="evenodd" clip-rule="evenodd"
                        d="M4.5 8.67188C4.5 4.67875 7.89479 1.5 12 1.5C16.1052 1.5 19.5 4.67875 19.5 8.67188C19.5 10.923 18.2829 13.7082 16.9495 16.1155C15.5963 18.559 14.0377 20.7603 13.2019 21.8891C13.064 22.0776 12.8837 22.231 12.6755 22.337C12.4663 22.4435 12.2348 22.499 12 22.499C11.7652 22.499 11.5337 22.4435 11.3245 22.337C11.1163 22.231 10.9359 22.0776 10.798 21.889C9.96224 20.7598 8.40373 18.5577 7.05043 16.1139C5.71719 13.7064 4.5 10.9214 4.5 8.67188ZM12 3C8.65209 3 6 5.57656 6 8.67188C6 10.5004 7.03281 12.9859 8.36265 15.3872C9.66887 17.746 11.1818 19.886 12 20.9917C12.8181 19.8865 14.3311 17.7473 15.6374 15.3888C16.9671 12.9878 18 10.5021 18 8.67188C18 5.57656 15.3479 3 12 3Z"
                        fill="#F4E07A" />
                      <path fill-rule="evenodd" clip-rule="evenodd"
                        d="M12 7.5C11.1716 7.5 10.5 8.17157 10.5 9C10.5 9.82843 11.1716 10.5 12 10.5C12.8284 10.5 13.5 9.82843 13.5 9C13.5 8.17157 12.8284 7.5 12 7.5ZM9 9C9 7.34315 10.3431 6 12 6C13.6569 6 15 7.34315 15 9C15 10.6569 13.6569 12 12 12C10.3431 12 9 10.6569 9 9Z"
                        fill="#F4E07A" />
                    </svg>
                  </div>
                </div>

                <div class="form__group">
                  <label for="startDate" class="form__label" id="startDateLabel">Start Date</label>
                  <input type="date" class="form__field " id="startDate" required="true"
                    onchange="hideLabel('startDate')">
                </div>

                <div class="form__group">
                  <label for="endDate" class="form__label" id="endDateLabel">End Date</label>
                  <input type="date" class="form__field" id="endDate" required="true" onchange="hideLabel('endDate')">
                </div>
                <div class="form__group">
                  <button type="button" class="btn btn-yellow">Search</button>
                </div>
              </div>
            </div>
          </div>
          <div class="section_home_right bg-video-wrap">
            <video src="https://designsupply-web.com/samplecontent/vender/codepen/20181014.mp4" loop muted autoplay>
            </video>
            <div class="overlay">
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- ========== End home section ========== -->

    <!-- ========== Start Experience Section ========== -->
    <div class="container section_experience">
      <div class="section_row_one">
        <div class="section_experience_one">
          <div class="section_experience_bar">
            <hr class="section_bar_yellow">
          </div>
          <div>
            <p class="section_content uppercase">Offering you a wide array of activities, each providing a </p>
            <h1 class="section_title_large">
              Unique & Diverse Experience
            </h1>
          </div>
        </div>
        <div class="section_experience_two section_position" data-aos="flip-down" data-aos-duration="2000"
          data-aos-easing="ease-in-out"><img src="/images/experience img/road trips.png" alt="Loading"
            class="section_experience_img">
          <div class="section_experience_items">
            <h3 class="section_experience_title">Road Trips</h3>
            <img src="/images/experience img/arrow-down-left.svg" alt="" class="arrow-down-left-up">
          </div>
        </div>
        <div class="section_experience_three section_position" data-aos="flip-up" data-aos-duration="2000"
          data-aos-easing="ease-out-back">
          <img src="/images/experience img/sand tours.png" alt="loading" class="section_experience_img">
          <div class="section_experience_items">
            <h3 class="section_experience_title">Sand Tours</h3>
            <img src="/images/experience img/arrow-down-left.svg" alt="" class="arrow-down-left-up">
          </div>
        </div>
      </div>
      <div class="section_row_two">
        <div class="section_experience_four section_position" data-aos="flip-right" data-aos-duration="1000"
          data-aos-easing="ease-in-back"><img src="/images/experience img/airplane.png" alt="loading"
            class="section_experience_img">
          <div class="section_experience_items">
            <h3 class="section_experience_title">Air Tours</h3>
            <img src="/images/experience img/arrow-down-left.svg" alt="" class="arrow-down-left">
          </div>
        </div>
        <div class="section_experience_five  section_position" data-aos="flip-up" data-aos-duration="1500"
          data-aos-easing="ease-in-back"><img src="/images/experience img/water boats.jpg" alt="loading"
            class="section_experience_img">
          <div class="section_experience_items">
            <h3 class="section_experience_title">Water Boats</h3>
            <img src="/images/experience img/arrow-down-left.svg" alt="" class="arrow-down-left">
          </div>
        </div>
        <div class="section_experience_six section_position" data-aos="flip-left" data-aos-duration="1000"
          data-aos-easing="ease-in-back"><img src="/images/experience img/party.png" alt="loading"
            class="section_experience_img">
          <div class="section_experience_items">
            <h3 class="section_experience_title">Party Moods</h3>
            <img src="/images/experience img/arrow-down-left.svg" alt="" class="arrow-down-left">
          </div>
        </div>
      </div>
    </div>
    <!-- ========== End Experience Section ========== -->

    <!-- ========== Start about_Section ========== -->
    <div class="section_about">
      <div class="container">
        <div class="section_about_box">
          <div class="section_about_left">
            <div class="section_about_head"><span class="section_about_line">
                <hr class="section_bar_white">
              </span>
              <span class="section_head_title">
                <%= aboutData.title %>
              </span>
            </div>
            <div class="section_title">
              About Us
            </div>
            <p class="section_content section_content_left">
              <%= aboutData.description %>
            </p>
            <button type="button" class="btn btn-yellow">Book Visit</button>
          </div>
          <div class="section_about_right">
            <div class="section_about_card card_active" data-aos="zoom-in-up" data-aos-duration="1000">
              <h1 class="about_card_title">Feature Title</h2>
                <img src="/images/about img/vip.svg" alt="" class="about_card_img">
                <p class="about_card_content">lorem data is now shown in below</p>
            </div>
            <div class="section_about_card card_active" data-aos="zoom-in-up" data-aos-duration="1000">
              <h1 class="about_card_title">Feature Title</h2>
                <img src="/images/about img/star_box.svg" alt="" class="about_card_img">
                <p class="about_card_content">lorem data is now shown in below</p>
            </div>
            <div class="section_about_card card_active" data-aos="zoom-in-up" data-aos-duration="1000">
              <h1 class="about_card_title">Feature Title</h2>
                <img src="/images/about img/location.svg" alt="" class="about_card_img">
                <p class="about_card_content">lorem data is now shown in below</p>
            </div>
            <div class="section_about_card card_active" data-aos="zoom-in-up" data-aos-duration="1000">
              <h1 class="about_card_title">Feature Title</h2>
                <img src="/images/about img/camel.svg" alt="" class="about_card_img">
                <p class="about_card_content">lorem data is now shown in below</p>
            </div>
          </div>
        </div>
      </div>
      <div class="about_gradient_topright"></div>
      <div class="about_gradient_bottomleft"></div>
    </div>
    <!-- ========== End about_Section ========== -->

    <!-- ========== Start Destination Section ========== -->
    <div class="section_destination">
      <div class="container">
        <div class="section_destination_box">
          <div class="section_destination_left">
            <div class="section_about_head"><span class="section_about_line">
